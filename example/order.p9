(all X all Y ((less(X, Y) & less(Y, X)) -> (X = Y))) # label(antisymmetry).
(all X all Y all Z ((less(X, Y) & less(Y, Z)) -> less(X, Y))) # label(transitivity).
(all X all Y (less(X, Y) | less(Y, X))) # label(totality).
(all X (less(X, succ(X)) & (all Y (less(Y, X) | less(succ(X), Y))))) # label(successor).
(all X ((pred(succ(X)) = X) & (succ(pred(X)) = X))) # label(predecessor).
